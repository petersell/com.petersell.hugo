{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "blog",
      "pageBundle": false,
      "previewPath": "blog",
      "filePrefix": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Cover",
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "author",
          "name": "author",
          "type": "string",
          "default": "Andreas Petersell",
          "hidden": "true"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}"
        },
        {
          "title": "lastMod",
          "name": "lastMod",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "fmContentType",
          "name": "fmContentType",
          "type": "string",
          "hidden": "true"
        }
      ]
    }
    {
      "name": "status",
      "pageBundle": false,
      "previewPath": "status",
      "filePrefix": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Cover",
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "author",
          "name": "author",
          "type": "string",
          "default": "Andreas Petersell",
          "hidden": "true"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}"
        },
        {
          "title": "lastMod",
          "name": "lastMod",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "fmContentType",
          "name": "fmContentType",
          "type": "string",
          "hidden": "true"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.taxonomy.alignFilename": true,
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/content/blog"
    },
    {
      "title": "status",
      "path": "[[workspace]]/content/status"
    }
  ],
  "frontMatter.git.enabled": false,
  "frontMatter.website.host": "http://localhost:1313/"
}